<h1>My contacts-app</h1>
<table class="table table-bordered">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Phone Number</th>
  </tr>
  <% @contacts.each do |contact| %>
    <tr>
      <td><a href="/contacts/<%= contact.id %>"><%= contact.first_name %></a></td>
      <td><%= contact.last_name %></td>
      <td><%= contact.email %></td>
      <td><%= contact.phone_number %></td>
    </tr>
  <% end %>

</table>